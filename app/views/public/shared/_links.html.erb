<div class="container">
<%- if controller_name != 'sessions' %>
  <h3>既に登録薄みの方</h3>
  <%= link_to "こちら", new_session_path(resource_name) %><span>からログインしてください。</span>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <h4>会員登録がお済みでない方</h4>
  <%= link_to "こちら", new_registration_path(resource_name) %><span>から新規登録を行なってください。</span>
<% end %>
</div>